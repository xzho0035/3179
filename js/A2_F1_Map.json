{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "The Australia's Forest Cover Ratio in 2021",
    "width": 600,
    "height": 400,
    "projection": {"type": "mercator"},
    "data": {
      "url": "js/STE_2016_AUST.json",
      "format": {"type": "topojson", "feature": "STE_2016_AUST"}
    },
    "transform": [
      {
        "lookup": "properties.STE_NAME16",
        "from": {
          "data": {
            "url": "data/ForetCover.csv"
          },
          "key": "State",
          "fields": ["Forest Cover"]
        }
      }
    ],
    "mark": {"type": "geoshape", "stroke": "black"},
    "encoding": {
      "color": {
        "field": "Forest Cover",
        "type": "quantitative",
        "scale": {"type": "threshold",
          "range": ["#FFD700", "#FFFFF0", "#F0FFF0", "#90EE90", "#32CD32", "#2E8B57", "#006400"],
        "domain": [0.00, 0.10, 0.20, 0.30 , 0.40, 0.50, 0.60]}
      },
      "tooltip": [
        {"field": "properties.STE_NAME16", "type": "nominal", "title": "State"},
        {"field": "Forest Cover", "type": "quantitative"}
      ]
    }
  }